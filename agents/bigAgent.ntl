<< name: meetingTranscript, prompt: true, desc: "Meeting transcript" >>
<< name: summary, prompt: false >>
display:
<< name: summary, prompt: false >><< name: nextTasks, prompt: false >><< name: email, prompt: false >><< name: calendar, prompt: false >><< name: blockers, prompt: false >>=>{{ variable  | name: "transcript" | mode: "append" }}
{{ maistro  | template: "FollowUpSummarizer" | ntl: "<< name: summary, prompt: false >> " }}=>{{ variable  | name: "summary" }}
{{ maistro  | template: "FollowUpNextTasks" }}=>{{ variable  | name: "nextTasks" }}
{{ maistro  | template: "FollowUpBlockers" }}=>{{ variable  | name: "blockers" }}
{{ maistro  | template: "FollowUpEmail" }}=>{{ variable  | name: "email" }}
{{ maistro  | template: "FollowUpCalendar" }}=>{{ variable  | name: "calendar" | mode: "append" }}
{
  "summary": << name: summary, prompt: false >>,
  "nextTasks": << name: nextTasks, prompt: false >>,
  "blockers": << name: blockers, prompt: false >>,
  "email": << name: email, prompt: false >>,
  "calendar": << name: calendar, prompt: false >>,
}